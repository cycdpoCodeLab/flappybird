doctype html
html(lang='en')
  head
    include includes/meta.pug
    title Flappy Bird
    base(href="./")

    //- icon
    link(rel="shortcut icon", href=require('../favicon.ico'))

    //- webpack style
    each _style in htmlWebpackPlugin.files.css
      link(href=_style, rel="stylesheet")

  body
    each _js in htmlWebpackPlugin.files.js
      script(src=_js)

